<div class="main">
  <div class="inner">
    <div class="form__wrapper">
      <h2 class="page-heading">新規プロトタイプ投稿</h2>

        <% if @prototype.errors.any? %>
          <div class="errors">
            <ul>
              <% @prototype.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
              <% end %>
            </ul>
          </div>
        <% end %>
        
        <%= render 'form', prototype: @prototype %>
    </div>
  </div>
</div>
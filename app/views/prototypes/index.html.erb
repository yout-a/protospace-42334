<main class="main">
  <div class="inner">

    <% if user_signed_in? %>
      <div class="greeting">
        こんにちは、
        <%= link_to "#{current_user.name}さん", user_path(current_user), class: :greeting__link %>
      </div>
    <% end %>

    <div class="prototypes-list">
      <div class="card__wrapper">
        <% @prototypes.each do |prototype| %>
          <%= render 'prototype', prototype: prototype %>
        <% end %>
      </div>
    </div>
  </div>
</main>